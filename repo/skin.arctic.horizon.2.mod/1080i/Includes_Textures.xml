<?xml version="1.0" encoding="UTF-8"?>
<includes>

    <include name="Texture_Shadow_20">
        <param name="colordiffuse" default="shadow_hard" />
        <definition>
            <include content="Dimension_Padding"><param name="pad" value="-20" /></include>
            <bordersize>20</bordersize>
            <bordertexture border="32" colordiffuse="$PARAM[colordiffuse]">shadows/shadow-20.png</bordertexture>
        </definition>
    </include>

    <include name="Texture_Shadow_40">
        <param name="colordiffuse" default="shadow_hard" />
        <definition>
            <include content="Dimension_Padding"><param name="pad" value="-40" /></include>
            <bordersize>40</bordersize>
            <bordertexture border="52" colordiffuse="$PARAM[colordiffuse]">shadows/shadow-40.png</bordertexture>
        </definition>
    </include>

    <include name="Texture_Glow_40">
        <param name="colordiffuse" default="shadow_hard" />
        <param name="affix">v</param>
        <definition>
            <include content="Dimension_Padding"><param name="pad" value="-40" /></include>
            <bordersize>40</bordersize>
            <bordertexture border="52" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/glow.png</bordertexture>
        </definition>
    </include>

    <include name="Texture_Shadow_Provider">
        <include content="Dimension_Padding"><param name="pad" value="-40" /></include>
        <bordersize>40</bordersize>
        <bordertexture colordiffuse="shadow_hard">shadows/circle_provider.png</bordertexture>
    </include>

    <include name="Texture_Highlight">
        <texture colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/main.png</texture>
    </include>

    <include name="Texture_Highlight_Focus">
        <texturefocus colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/main.png</texturefocus>
        <alttexturefocus colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/main.png</alttexturefocus>
    </include>

    <include name="Texture_Highlight_ToggleButton_FakeFocus">
        <texturenofocus />
        <texturefocus colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/main.png</texturefocus>
        <alttexturefocus colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/main.png</alttexturefocus>
        <alttexturenofocus colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/main.png</alttexturenofocus>
    </include>

    <include name="Texture_Highlight_ToggleButton_Settings">
        <texturefocus colordiffuse="$VAR[ColorSettingsToggleButton]">special://skin/extras/textures/main.png</texturefocus>
        <alttexturefocus colordiffuse="$VAR[ColorSettingsToggleButton]">special://skin/extras/textures/main.png</alttexturefocus>
    </include>

    <include name="Texture_Scrollbar_H">
        <texturesliderbarfocus border="5" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/scrollh.png</texturesliderbarfocus>
    </include>
    <include name="Texture_Scrollbar_V">
        <texturesliderbarfocus border="5" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/scrollv.png</texturesliderbarfocus>
    </include>

    <include name="Texture_Star_Full">
        <texture colordiffuse="ffeebb00">flags/star10.png</texture>
    </include>

    <include name="Texture_Star_Half">
        <texture colordiffuse="ffeebb00">flags/star5.png</texture>
    </include>

    <include name="Texture_Dialog">
        <texture border="16" colordiffuse="dialog_bg_100">common/dialog.png</texture>
    </include>

    <include name="Texture_MainMenu_Highlight_H">
        <texture border="80" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/menumain.png</texture>
    </include>
    <include name="Texture_Box_Highlight_H">
        <texture border="12" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/box.png</texture>
    </include>
    <include name="Texture_Box_Highlight_V">
        <texture border="12" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/box.png</texture>
    </include>
    <include name="Texture_Box_Highlight_Focus_H">
        <texturefocus border="12" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/box.png</texturefocus>
    </include>
    <include name="Texture_Menu_Highlight_H">
        <texture border="40" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/menu.png</texture>
    </include>
    <include name="Texture_Circle_Highlight_V">
        <texture flipy="true" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/circle.png</texture>
    </include>
    <include name="Texture_SelectBox">
        <texture border="20" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/selectbox.png</texture>
    </include>
    <include name="Texture_SelectBox_Thin">
        <texture border="20" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/selectbox_thin.png</texture>
    </include>
    <include name="Texture_Glow_Highlight">
        <texture border="52" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/glow.png</texture>
    </include>

    <include name="Texture_Poster_Back">
        <texture colordiffuse="main_fg_12" border="12">common/box.png</texture>
    </include>

    <include name="Texture_KeyboardButton">
        <texturenofocus colordiffuse="dialog_fg_12" border="40">common/menu.png</texturenofocus>
        <include content="Object_TextureFocus">
            <param name="border">40</param>
            <param name="texture">special://skin/extras/textures/circle.png</param>
            <param name="colordiffuse">$VAR[ColorHighlight]</param>
        </include>
    </include>

    <include name="Texture_Button">
        <texturenofocus colordiffuse="dialog_fg_12" border="40">common/menu.png</texturenofocus>
        <include content="Object_TextureFocus">
            <param name="border">40</param>
            <param name="texture">special://skin/extras/textures/menu.png</param>
            <param name="colordiffuse">$VAR[ColorHighlight]</param>
        </include>
    </include>

    <include name="Texture_ToggleButton">
        <alttexturenofocus colordiffuse="dialog_fg_12" border="40">common/menu.png</alttexturenofocus>
        <include content="Object_AltTextureFocus">
            <param name="border">40</param>
            <param name="texture">special://skin/extras/textures/menu.png</param>
            <param name="colordiffuse">$VAR[ColorHighlight]</param>
        </include>
    </include>

</includes>
