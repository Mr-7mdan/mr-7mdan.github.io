        <texture diffuse="$PARAM[diffuse]" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/main.png</texture>
    </include>
    <include name="Texture_Highlight_Focus">
        <texturefocus>special://skin/extras/textures/main.png</texturefocus>
        <alttexturefocus>special://skin/extras/textures/main.png</alttexturefocus>
    </include>
    <include name="Texture_Highlight_Button_FakeFocus_V">
        <texturefocus border="40">special://skin/extras/textures/menu.png</texturefocus>
        <texturenofocus border="40">special://skin/extras/textures/menu.png</texturenofocus>
    </include>
    <include name="Texture_Highlight_Button_FakeFocus_H">
        <texturefocus border="40">special://skin/extras/textures/menu.png</texturefocus>
        <texturenofocus border="40">special://skin/extras/textures/menu.png</texturenofocus>
    </include>
    <include name="Texture_Highlight_ToggleButton_FakeFocus_H">
        <texturenofocus/>
        <texturefocus border="40">special://skin/extras/textures/menu.png</texturefocus>
