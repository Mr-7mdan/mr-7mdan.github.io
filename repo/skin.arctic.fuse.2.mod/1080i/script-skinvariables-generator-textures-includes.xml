        <alttexturefocus border="40">special://skin/extras/textures/menu.png</alttexturefocus>
        <alttexturenofocus border="40">special://skin/extras/textures/menu.png</alttexturenofocus>
    </include>
    <include name="Texture_Highlight_ToggleButton_Settings">
        <texturefocus colordiffuse="$VAR[ColorSettingsToggleButton_Script]" border="40">special://skin/extras/textures/menu.png</texturefocus>
        <alttexturefocus colordiffuse="$VAR[ColorSettingsToggleButton_Script]" border="40">special://skin/extras/textures/menu.png</alttexturefocus>
    </include>
    <include name="Texture_Highlight_Underline_H">
        <texturefocus border="4">special://skin/extras/textures/underline.png</texturefocus>
