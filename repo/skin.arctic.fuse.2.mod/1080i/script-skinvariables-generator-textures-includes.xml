    </include>
    <include name="Texture_Scrollbar_SliderBar_H">
        <texturesliderbar border="4">special://skin/extras/textures/scrollh.png</texturesliderbar>
    </include>
    <include name="Texture_Scrollbar_V">
        <texturesliderbarfocus border="4">special://skin/extras/textures/scrollv.png</texturesliderbarfocus>
    </include>
    <include name="Texture_Scrollbar_SliderBar_V">
        <texturesliderbar border="4">special://skin/extras/textures/scrollv.png</texturesliderbar>
    </include>
    <include name="Texture_MainMenu_Highlight_ToggleButton_FakeFocus">
        <texturenofocus/>
        <texturefocus border="80">special://skin/extras/textures/menumain.png</texturefocus>
        <alttexturefocus border="80">special://skin/extras/textures/menumain.png</alttexturefocus>
        <alttexturenofocus border="80">special://skin/extras/textures/menumain.png</alttexturenofocus>
    </include>
    <include name="Texture_MainMenu_Highlight_V">
        <texture border="80">special://skin/extras/textures/menumain.png</texture>
    </include>
    <include name="Texture_MainMenu_Highlight_H">
        <texture border="80">special://skin/extras/textures/menumain.png</texture>
    </include>
    <include name="Texture_Box_Highlight_H">
        <texture border="20">special://skin/extras/textures/box.png</texture>
    </include>
    <include name="Texture_Box_Highlight_V">
        <texture border="20">special://skin/extras/textures/box.png</texture>
    </include>
    <include name="Texture_Box_Highlight_Focus_H">
        <texturefocus border="20">special://skin/extras/textures/box.png</texturefocus>
    </include>
    <include name="Texture_Menu_Highlight_H">
        <param name="border">40</param>
        <definition>
            <texture border="$PARAM[border]">special://skin/extras/textures/menu.png</texture>
        </definition>
    </include>
    <include name="Texture_Menu_Highlight_V">
        <param name="border">40</param>
        <definition>
            <texture border="$PARAM[border]">special://skin/extras/textures/menu.png</texture>
        </definition>
    </include>
    <include name="Texture_Circle_Highlight_H">
        <texture>special://skin/extras/textures/circle.png</texture>
    </include>
    <include name="Texture_Circle_Highlight_V">
        <texture>special://skin/extras/textures/circle.png</texture>
    </include>
    <include name="Texture_Circle_Highlight_Focus_V">
        <texturefocus>special://skin/extras/textures/circle.png</texturefocus>
    </include>
    <include name="Texture_Circle_Highlight_NoFocus_V">
        <texturenofocus>special://skin/extras/textures/circle.png</texturenofocus>
    </include>
    <include name="Texture_CircleButtonDialog_Highlight_V">
        <texture>special://skin/extras/textures/circlebuttondialog.png</texture>
    </include>
    <include name="Texture_CircleButtonDialog_Highlight_Focus_V">
        <texturefocus border="80">special://skin/extras/textures/circlebuttondialog.png</texturefocus>
    </include>
    <include name="Texture_SelectBox_Thin">
        <texture border="20">special://skin/extras/textures/selectbox_thin.png</texture>
    </include>
    <include name="Texture_KeyboardButton">
        <texturefocus>special://skin/extras/textures/circle.png</texturefocus>
    </include>
    <include name="Texture_Button_X">
        <texture border="60,0,0,0">special://skin/extras/textures/menu_h_x.png</texture>
        <texturefocus border="60,0,0,0">special://skin/extras/textures/menu_h_x.png</texturefocus>
    </include>
    <include name="Texture_Button_Y">
        <texture>special://skin/extras/textures/menu_h_y.png</texture>
        <texturefocus>special://skin/extras/textures/menu_h_y.png</texturefocus>
    </include>
    <include name="Texture_Button_Z">
        <texture border="0,0,40,0">special://skin/extras/textures/menu_h_z.png</texture>
        <texturefocus border="0,0,40,0">special://skin/extras/textures/menu_h_z.png</texturefocus>
    </include>
    <include name="Texture_Button">
        <texturenofocus colordiffuse="$VAR[Color_DialogBorder]" border="40">common/menu.png</texturenofocus>
        <texturefocus border="40">special://skin/extras/textures/menu.png</texturefocus>
    </include>
    <include name="Texture_ToggleButton">
        <alttexturenofocus colordiffuse="main_bg_12" border="40">common/menu.png</alttexturenofocus>
        <alttexturefocus border="40">special://skin/extras/textures/menu.png</alttexturefocus>
    </include>
    <include name="Defs_BusySpinner_Image">
        <param name="spinspinner">true</param>
        <definition>
            <description>Busy animation</description>
            <texture>dialog/loading-spinner.png</texture>
            <aspectratio>keep</aspectratio>
            <animation effect="fadediffuse" condition="$PARAM[spinspinner]" start="ffffffff" end="ffffffff" pulse="true" time="1000">Conditional</animation>
            <animation effect="rotate" condition="$PARAM[spinspinner]" start="0" end="360" loop="true" time="1000" center="auto">Conditional</animation>
        </definition>
    </include>
</includes>