        <alttexturenofocus border="4">special://skin/extras/textures/underline.png</alttexturenofocus>
        <texturenofocus/>
    </include>
    <include name="Texture_Highlight_Scrollbar_H">
        <texture border="4">special://skin/extras/textures/scrollh.png</texture>
    </include>
    <include name="Texture_Highlight_Scrollbar_V">
        <texture border="4">special://skin/extras/textures/scrollh.png</texture>
    </include>
    <include name="Texture_Scrollbar_H">
