        <texturenofocus colordiffuse="$VAR[Color_DialogBorder]" border="40">common/menu.png</texturenofocus>
        <texturefocus border="40" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/menu.png</texturefocus>
    </include>

    <include name="Texture_ToggleButton">
        <alttexturenofocus colordiffuse="main_bg_12" border="40">common/menu.png</alttexturenofocus>
        <alttexturefocus border="40" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/menu.png</alttexturefocus>
