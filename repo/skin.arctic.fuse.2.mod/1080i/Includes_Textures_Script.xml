<?xml version="1.0" encoding="UTF-8"?>
<includes>

    <!-- Texture Maker -->
    <include name="Texture_Highlight">
        <texture diffuse="$PARAM[diffuse]" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/main.png</texture>
    </include>
    <include name="Texture_Highlight_Focus">
        <texturefocus colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/main.png</texturefocus>
        <alttexturefocus colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/main.png</alttexturefocus>
    </include>
    <include name="Texture_Highlight_Button_FakeFocus_V">
        <texturefocus border="40" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/menu.png</texturefocus>
        <texturenofocus border="40" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/menu.png</texturenofocus>
    </include>
    <include name="Texture_Highlight_Button_FakeFocus_H">
        <texturefocus border="40" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/menu.png</texturefocus>
