    </include>
    <include name="Texture_Highlight_Scrollbar_V">
        <texture border="4" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/scrollv.png</texture>
    </include>
    <include name="Texture_Scrollbar_H">
        <texturesliderbarfocus border="4" colordiffuse="$VAR[ColorHighlight]">special://skin/extras/textures/scrollh.png</texturesliderbarfocus>
